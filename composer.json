{
    "name": "glash-gnome/zend-ext",
    "description": "provides tools for working with PHP extension",
    "version": "0.1.0",
    "type": "library",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/gtkphp/zend-c.git"
        }
    ],
    "require": {
        "php": ">=7.2.6",
        "zendframework/zend-loader": "2.6.*@dev",
        "zendframework/zend-filter": "2.8.*@dev",
        "zendframework/zend-servicemanager": "3.3.*@dev",
        "zendframework/zend-view": "2.10.*@dev",
        "zendframework/zend-stdlib": "3.2.*@dev",
        "gtkphp/zend-c": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2@dev"
    },
    "autoload": {
        "psr-4": {
            "Zend\\Ext\\": "src/",
            "Zend\\C\\": "src/",
            "Zend\\View\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Ext\\": "test/"
        }
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sergio",
            "email": "dev-5312910@php.net"
        }
    ],
    "minimum-stability": "dev"
}
